{
  "name": "main-street-project-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && webpack --mode production",
    "build-storybook": "build-storybook -c .storybook -o ./storybook-static",
    "clean": "rimraf ./build",
    "deploy": "firebase deploy",
    "dev": "webpack-dev-server --hot --silent",
    "lint": "npm run format -- --check",
    "format": "prettier-standard 'src/**/*.{js,jsx,css,md,json}'",
    "precommit": "lint-staged && npm test",
    "start": "firebase emulators:start",
    "start:client": "firebase emulators:start --only hosting",
    "storybook": "start-storybook -c .storybook",
    "test": "jest --silent --coverage",
    "test:ci": "jest --silent"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,css,md,json}": [
      "prettier-standard"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "axios": "^0.21.1",
    "history": "^4.10.1",
    "pcln-autocomplete": "^3.0.0",
    "pcln-design-system": "^3.5.3",
    "pcln-icons": "^3.1.1",
    "pcln-modal": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-collapse": "^5.0.1",
    "react-dom": "^16.13.1",
    "react-loading-skeleton": "^2.0.1",
    "react-router-dom": "^5.1.2",
    "react-swipeable": "^5.5.1",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^6.0.25",
    "@storybook/react": "^6.0.25",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "assets-webpack-plugin": "^3.9.12",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.4.2",
    "firebase-tools": "^8.10.0",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.3.0",
    "jest": "^25.2.3",
    "lint-staged": "^10.4.2",
    "prettier-standard": "^16.4.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
